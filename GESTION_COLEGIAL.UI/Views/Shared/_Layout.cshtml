<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mi aplicación ASP.NET</title>
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Content/img/favicon.ico">

    <!-- App css -->
    <link rel="icon" type="image/x-icon" href="~/Content/img/favicon.ico" />
    <link href="~/Content/css/loader.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/main.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/scrollspyNav.css" rel="stylesheet" type="text/css" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="https://fonts.googleapis.com/css?family=Quicksand:400,500,600,700&display=swap" rel="stylesheet">
    <link href="~/Content/plugins/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" />
    <link href="~/Content/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/plugins.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM STYLES -->
    <link href="~/Content/plugins/apex/apexcharts.css" rel="stylesheet" type="text/css">
    <link href="~/Content/css/dashboard/dash_1.css" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGINS/CUSTOM STYLES -->
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

    @* ladda *@
    <link href="~/Content/plugins/ladda/ladda-themeless.css" rel="stylesheet" />

    @* toastr *@
    <link href="~/Content/plugins/toastr/toastr.css" rel="stylesheet" />

</head>
<body>

    <!-- BEGIN LOADER -->
    <div id="load_screen">
        <div class="loader">
            <div class="loader-content">
                <div class="spinner-grow align-self-center"></div>
            </div>
        </div>
    </div>
    <!--  END LOADER -->
    <!--  BEGIN HEADER -->
    @Html.Partial("~/Views/Shared/_header.cshtml");
    <!--  END HEADER -->
    <!-- BEGIN CONTENT -->
    <div class="main-container" id="container">
        <!--  BEGIN SIDEBAR -->
        @Html.Partial("~/Views/Shared/_sidebar.cshtml");
        <!--  END SIDEBAR -->
        <div id="content" class="main-content">
            <div class="layout-px-spacing">
                @RenderBody()
            </div>
            <!--  BEGIN  FOOTER -->
            @Html.Partial("~/Views/Shared/_footer.cshtml");
            <!--  END FOOTER -->
        </div>
    </div>
    <!-- END CONTENT -->
    <!-- BEGIN GLOBAL MANDATORY SCRIPTS -->
    <script src="~/Content/js/libs/jquery-3.1.1.min.js"></script>
    <script src="~/Content/plugins/bootstrap/js/popper.min.js"></script>
    <script src="~/Content/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Content/js/app.js"></script>
    <script src="~/Content/js/loader.js"></script>
    @*<script>
        $(document).ready(function () {
            App.init();
        });
    </script>*@
    <script src="~/Content/js/custom.js"></script>
    <!-- END GLOBAL MANDATORY SCRIPTS -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
    <script src="~/Content/plugins/apex/apexcharts.min.js"></script>
    <script src="~/Content/js/dashboard/dash_1.js"></script>
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    <!-- jQuery  -->
    @*<script src="~/Content/js/jquery.min.js"></script>
        <script src="~/Content/js/jquery-ui.min.js"></script>
        <script src="~/Content/plugins/bootstrap-4.5.3-dist/js/bootstrap.bundle.js"></script>*@

    @*<script src="~/Content/js/waves.js"></script>
        <script src="~/Content/js/feather.min.js"></script>
        <script src="~/Content/js/jquery.slimscroll.min.js"></script>
        <script src="~/Content/plugins/apexcharts/apexcharts.min.js"></script>*@

    @* jquery-unobtrusive *@
    <script src="~/Content/plugins/jquery-validation/jquery.validate.js"></script>
    <script src="~/Content/plugins/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/Content/plugins/jquery-validation-unobtrusive/jquery.unobtrusive-ajax.js"></script>

    @* toastr *@
    <script src="~/Content/plugins/toastr/toastr.min.js"></script>

    @* ladda *@
    <script src="~/Content/plugins/ladda/spin.min.js"></script>
    <script src="~/Content/plugins/ladda/ladda.min.js"></script>

    <script src="~/Content/js/components/alert.js"></script>
    @*<script src="~/Content/js/main.js"></script>*@
    @*<script src="~/Content/js/app.js"></script>*@

    @*<script src="~/Content/js/components/navbar.js"></script>*@

    <!-- Required datatable js -->
    <script src="~/Content/plugins/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/plugins/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons-bs4/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Content/plugins/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/plugins/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
    @*<script src="~/Content/plugins/summernote/summernote-bs4.min.js"></script>*@

    <script src="~/Content/plugins/datatables/jszip.min.js"></script>
    @*<script src="~/Content/plugins/jszip/jszip.min.js"></script>*@
    <script src="~/Content/plugins/datatables/pdfmake.min.js"></script>
    @*<script src="~/Content/plugins/pdfmake/build/pdfmake.min.js"></script>*@
    <script src="~/Content/plugins/datatables/vfs_fonts.js"></script>
    @*<script src="~/Content/plugins/pdfmake/build/vfs_fonts.js"></script>*@
    <script src="~/Content/plugins/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/Content/plugins/datatables.net-buttons/js/buttons.colVis.min.js"></script>

    <script src="~/Content/js/components/datatable.init.js"></script>
    <script src="~/Content/js/components/catalogs.js"></script>
    @*<script src="~/Content/js/components/datatable.init.js"></script>*@


    <!-- App js -->
    @RenderSection("Scripts", required: false)
</body>
</html>
