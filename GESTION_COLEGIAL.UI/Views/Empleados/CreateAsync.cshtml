@model GESTION_COLEGIAL.Business.Models.EmpleadoViewModel
@{
    ViewBag.Title = "Crear";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Styles {
    <link href="~/Content/css/form-empleados.css" rel="stylesheet" />
    <link href="~/Content/css/catalog-links.css" rel="stylesheet" />
}


<div class="breadcrumb-wrapper-content ">
    <nav class="breadcrumb-style-one" aria-label="breadcrumb">
        <ol class="breadcrumb">

            <li class="breadcrumb-item"><a href="/">Inicio</a></li>
            <li class="breadcrumb-item"><a href="/Empleados/Index">Empleados</a></li>
            <li class="breadcrumb-item active" aria-current="page">Crear Empleado</li>
        </ol>
    </nav>
</div>


@using (Html.BeginForm("Save", "Empleados", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="page-header-empleado">
        <div class="header-content-wrapper">
            <div class="header-icon">
                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" fill="currentColor" viewBox="0 0 16 16">
                    <path d="M15 14s1 0 1-1-1-4-5-4-5 3-5 4 1 1 1 1h8Zm-7.978-1A.261.261 0 0 1 7 12.996c.001-.264.167-1.03.76-1.72C8.312 10.629 9.282 10 11 10c1.717 0 2.687.63 3.24 1.276.593.69.758 1.457.76 1.72l-.008.002a.274.274 0 0 1-.014.002H7.022ZM11 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4Zm3-2a3 3 0 1 1-6 0 3 3 0 0 1 6 0ZM6.936 9.28a5.88 5.88 0 0 0-1.23-.247A7.35 7.35 0 0 0 5 9c-4 0-5 3-5 4 0 .667.333 1 1 1h4.216A2.238 2.238 0 0 1 5 13c0-1.01.377-2.042 1.09-2.904.243-.294.526-.569.846-.816ZM4.92 10A5.493 5.493 0 0 0 4 13H1c0-.26.164-1.03.76-1.724.545-.636 1.492-1.256 3.16-1.275ZM1.5 5.5a3 3 0 1 1 6 0 3 3 0 0 1-6 0Zm3-2a2 2 0 1 0 0 4 2 2 0 0 0 0-4Z"/>
                </svg>
            </div>
            <div class="header-text">
                <h1 class="page-title">Registro de Empleados</h1>
                <p class="page-subtitle">Complete la información del empleado</p>
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row">
            <!-- Columna Principal: Información del Empleado -->
            <div class="col-lg-12">
                <div class="card-empleado">
                    <div class="card-header-empleado">
                        <div class="header-left">
                            <h2 class="card-title-empleado">Información del Empleado</h2>
                        </div>
                        <div class="header-right">
                            <div class="status-badge-empleado" id="statusBadge">
                                <span class="status-dot-empleado" id="statusDot"></span>
                                <span class="status-text-empleado" id="statusText">Activo</span>
                            </div>
                            <label class="switch-empleado">
                                @Html.CheckBoxFor(model => model.Per_EsActivo, new { @id = "statusToggle", @checked = "checked" })
                                <span class="slider-empleado"></span>
                            </label>
                        </div>
                    </div>

                    <div class="card-body-empleado">
                        @Html.HiddenFor(model => model.Emp_Id)
                        @Html.HiddenFor(model => model.Per_Id)

                        <div class="row">
                            <!-- Columna Código -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Emp_Codigo" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M4 1.5H3a2 2 0 0 0-2 2V14a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V3.5a2 2 0 0 0-2-2h-1v1h1a1 1 0 0 1 1 1V14a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V3.5a1 1 0 0 1 1-1h1v-1z"/>
                                            <path d="M9.5 1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-3a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5h3zm-3-1A1.5 1.5 0 0 0 5 1.5v1A1.5 1.5 0 0 0 6.5 4h3A1.5 1.5 0 0 0 11 2.5v-1A1.5 1.5 0 0 0 9.5 0h-3z"/>
                                        </svg>
                                        Código
                                    </label>
                                    @Html.EditorFor(model => model.Emp_Codigo, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "Ingrese código del empleado" } })
                                    @Html.ValidationMessageFor(model => model.Emp_Codigo, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <!-- Columna Título -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <div class="label-wrapper-empleado">
                                        <label for="Tit_Id" class="form-label-empleado">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                                <path d="M8.211 2.047a.5.5 0 0 0-.422 0l-7.5 3.5a.5.5 0 0 0 .025.917l7.5 3a.5.5 0 0 0 .372 0L14 7.014V13a1 1 0 0 0-1 1v2h3v-2a1 1 0 0 0-1-1V6.739l.686-.275a.5.5 0 0 0 .025-.917l-7.5-3.5Z"/>
                                                <path d="M4.176 9.032a.5.5 0 0 0-.656.327l-.5 1.7a.5.5 0 0 0 .294.605l4.5 1.8a.5.5 0 0 0 .372 0l4.5-1.8a.5.5 0 0 0 .294-.605l-.5-1.7a.5.5 0 0 0-.656-.327L8 10.466 4.176 9.032Z"/>
                                            </svg>
                                            Título
                                        </label>
                                        <a href="/Titulos/Index" target="_blank" class="catalog-link-empleado" title="Agregar nuevo título">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" fill="currentColor" viewBox="0 0 16 16">
                                                <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z"/>
                                            </svg>
                                            Agregar
                                        </a>
                                    </div>
                                    @Html.DropDownList("Tit_Id", Model.titulosList, "-- Seleccionar Título --", htmlAttributes: new { @class = "form-control form-input-empleado" })
                                    @Html.ValidationMessageFor(model => model.Tit_Id, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <!-- Columna Cargo -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <div class="label-wrapper-empleado">
                                        <label for="Car_Id" class="form-label-empleado">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                                <path d="M6.5 1A1.5 1.5 0 0 0 5 2.5V3H1.5A1.5 1.5 0 0 0 0 4.5v8A1.5 1.5 0 0 0 1.5 14h13a1.5 1.5 0 0 0 1.5-1.5v-8A1.5 1.5 0 0 0 14.5 3H11v-.5A1.5 1.5 0 0 0 9.5 1h-3zm0 1h3a.5.5 0 0 1 .5.5V3H6v-.5a.5.5 0 0 1 .5-.5zm1.886 6.914L15 7.151V12.5a.5.5 0 0 1-.5.5h-13a.5.5 0 0 1-.5-.5V7.15l6.614 1.764a1.5 1.5 0 0 0 .772 0zM1.5 4h13a.5.5 0 0 1 .5.5v1.616L8.129 7.948a.5.5 0 0 1-.258 0L1 6.116V4.5a.5.5 0 0 1 .5-.5z"/>
                                            </svg>
                                            Cargo
                                        </label>
                                        <a href="/Cargos/Index" target="_blank" class="catalog-link-empleado" title="Agregar nuevo cargo">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="14" height="14" fill="currentColor" viewBox="0 0 16 16">
                                                <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z"/>
                                            </svg>
                                            Agregar
                                        </a>
                                    </div>
                                    @Html.DropDownList("Car_Id", Model.cargosList, "-- Seleccionar Cargo --", htmlAttributes: new { @class = "form-control form-input-empleado" })
                                    @Html.ValidationMessageFor(model => model.Car_Id, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <!-- Columna Identidad -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_Identidad" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V4Zm2-1a1 1 0 0 0-1 1v1h14V4a1 1 0 0 0-1-1H2Zm13 4H1v5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V7Z"/>
                                            <path d="M2 10a1 1 0 0 1 1-1h1a1 1 0 0 1 1 1v1a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1v-1Z"/>
                                        </svg>
                                        Identidad
                                    </label>
                                    @Html.EditorFor(model => model.Per_Identidad, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "0000-0000-00000" } })
                                    @Html.ValidationMessageFor(model => model.Per_Identidad, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <!-- Columna Primer Nombre -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_PrimerNombre" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6Zm2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0Zm4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4Zm-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10c-2.29 0-3.516.68-4.168 1.332-.678.678-.83 1.418-.832 1.664h10Z"/>
                                        </svg>
                                        Primer Nombre
                                    </label>
                                    @Html.EditorFor(model => model.Per_PrimerNombre, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "Primer nombre" } })
                                    @Html.ValidationMessageFor(model => model.Per_PrimerNombre, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <!-- Columna Segundo Nombre -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_SegundoNombre" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3Zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6Z"/>
                                        </svg>
                                        Segundo Nombre
                                    </label>
                                    @Html.EditorFor(model => model.Per_SegundoNombre, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "Segundo nombre (opcional)" } })
                                    @Html.ValidationMessageFor(model => model.Per_SegundoNombre, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <!-- Columna Apellido Paterno -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_ApellidoPaterno" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M11 5a3 3 0 1 1-6 0 3 3 0 0 1 6 0ZM8 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4Zm.256 7a4.474 4.474 0 0 1-.229-1.004H3c.001-.246.154-.986.832-1.664C4.484 10.68 5.711 10 8 10c.26 0 .507.009.74.025.226-.341.496-.65.804-.918C9.077 9.038 8.564 9 8 9c-5 0-6 3-6 4s1 1 1 1h5.256Z"/>
                                            <path d="M12.5 16a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7Zm1.679-4.493-1.335 2.226a.75.75 0 0 1-1.174.144l-.774-.773a.5.5 0 0 1 .708-.708l.547.548 1.17-1.951a.5.5 0 1 1 .858.514Z"/>
                                        </svg>
                                        Apellido Paterno
                                    </label>
                                    @Html.EditorFor(model => model.Per_ApellidoPaterno, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "Apellido paterno" } })
                                    @Html.ValidationMessageFor(model => model.Per_ApellidoPaterno, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <!-- Columna Apellido Materno -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_ApellidoMaterno" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M11 5a3 3 0 1 1-6 0 3 3 0 0 1 6 0ZM8 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4Zm.256 7a4.474 4.474 0 0 1-.229-1.004H3c.001-.246.154-.986.832-1.664C4.484 10.68 5.711 10 8 10c.26 0 .507.009.74.025.226-.341.496-.65.804-.918C9.077 9.038 8.564 9 8 9c-5 0-6 3-6 4s1 1 1 1h5.256Z"/>
                                            <path d="M12.5 16a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7Zm1.679-4.493-1.335 2.226a.75.75 0 0 1-1.174.144l-.774-.773a.5.5 0 0 1 .708-.708l.547.548 1.17-1.951a.5.5 0 1 1 .858.514Z"/>
                                        </svg>
                                        Apellido Materno
                                    </label>
                                    @Html.EditorFor(model => model.Per_ApellidoMaterno, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "Apellido materno (opcional)" } })
                                    @Html.ValidationMessageFor(model => model.Per_ApellidoMaterno, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <!-- Columna Fecha Nacimiento -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_FechaNacimiento" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M4 .5a.5.5 0 0 0-1 0V1H2a2 2 0 0 0-2 2v1h16V3a2 2 0 0 0-2-2h-1V.5a.5.5 0 0 0-1 0V1H4V.5zM16 14V5H0v9a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2zM9.5 7h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5zm3 0h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5zM2 10.5a.5.5 0 0 1 .5-.5h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1zm3.5-.5h1a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-1a.5.5 0 0 1-.5-.5v-1a.5.5 0 0 1 .5-.5z"/>
                                        </svg>
                                        Fecha de Nacimiento
                                    </label>
                                    @Html.EditorFor(model => model.Per_FechaNacimiento, new { htmlAttributes = new { @class = "form-control form-input-empleado", @type = "date" } })
                                    @Html.ValidationMessageFor(model => model.Per_FechaNacimiento, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <!-- Columna Correo Electronico -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_CorreoElectronico" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V4Zm2-1a1 1 0 0 0-1 1v.217l7 4.2 7-4.2V4a1 1 0 0 0-1-1H2Zm13 2.383-4.708 2.825L15 11.105V5.383Zm-.034 6.876-5.64-3.471L8 9.583l-1.326-.795-5.64 3.47A1 1 0 0 0 2 13h12a1 1 0 0 0 .966-.741ZM1 11.105l4.708-2.897L1 5.383v5.722Z"/>
                                        </svg>
                                        Correo Electrónico
                                    </label>
                                    @Html.EditorFor(model => model.Per_CorreoElectronico, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "correo@ejemplo.com" } })
                                    @Html.ValidationMessageFor(model => model.Per_CorreoElectronico, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <!-- Columna Teléfono -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_Telefono" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M1 2a1 1 0 0 1 1-1h2.5a1 1 0 0 1 1 1v2a1 1 0 0 1-1 1H2v10h10v-2.5a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1V14a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2z"/>
                                            <path d="M15.854.146a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708 0L8.5 1.207V5.5a.5.5 0 0 1-1 0V.5a.5.5 0 0 1 .5-.5h5a.5.5 0 0 1 0 1H8.707l3.147 3.146a.5.5 0 0 1 .708 0l3-3z"/>
                                        </svg>
                                        Teléfono
                                    </label>
                                    @Html.EditorFor(model => model.Per_Telefono, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "0000-0000" } })
                                    @Html.ValidationMessageFor(model => model.Per_Telefono, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <!-- Columna Dirección -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_Direccion" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path d="M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.708L2 8.207V13.5A1.5 1.5 0 0 0 3.5 15h9a1.5 1.5 0 0 0 1.5-1.5V8.207l.646.647a.5.5 0 0 0 .708-.708L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293L8.707 1.5ZM13 7.207V13.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V7.207l5-5 5 5Z"/>
                                        </svg>
                                        Dirección
                                    </label>
                                    @Html.EditorFor(model => model.Per_Direccion, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "Dirección completa" } })
                                    @Html.ValidationMessageFor(model => model.Per_Direccion, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <!-- Columna Sexo -->
                            <div class="col-md-6">
                                <div class="form-group-empleado">
                                    <label for="Per_Sexo" class="form-label-empleado">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd" d="M11.5 1a.5.5 0 0 1 0-1h4a.5.5 0 0 1 .5.5v4a.5.5 0 0 1-1 0V1.707l-3.45 3.45A4 4 0 0 1 8.5 10.97V13H10a.5.5 0 0 1 0 1H8.5v1.5a.5.5 0 0 1-1 0V14H6a.5.5 0 0 1 0-1h1.5v-2.03a4 4 0 1 1 3.471-6.648L14.293 1H11.5zm-.997 4.346a3 3 0 1 0-5.006 3.309 3 3 0 0 0 5.006-3.31z"/>
                                        </svg>
                                        Sexo
                                    </label>
                                    @Html.EditorFor(model => model.Per_Sexo, new { htmlAttributes = new { @class = "form-control form-input-empleado", placeholder = "M / F" } })
                                    @Html.ValidationMessageFor(model => model.Per_Sexo, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>

                        <!-- Botones de acción -->
                        <div class="form-actions-empleado">
                            @Html.ActionLink("Cancelar", "Index", "Empleados", null, new { @class = "btn-secondary-empleado", @id = "cancelBtn" })
                            <button type="submit" class="btn-primary-empleado">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                    <path d="M2 1a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H9.5a1 1 0 0 0-1 1v7.293l2.646-2.647a.5.5 0 0 1 .708.708l-3.5 3.5a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L7.5 9.293V2a2 2 0 0 1 2-2H14a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h2.5a.5.5 0 0 1 0 1H2z"/>
                                </svg>
                                Guardar Empleado
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@section Scripts{
    <script src="~/Content/js/form-empleados.js"></script>
    <script src="~/Content/js/pages/empleados.js"></script>
    <script>
        Empleados.datatable({
            urlList: "@Url.Action("ListAsync")",
            urlInsert: "@Url.Action("CreateAsync")",
